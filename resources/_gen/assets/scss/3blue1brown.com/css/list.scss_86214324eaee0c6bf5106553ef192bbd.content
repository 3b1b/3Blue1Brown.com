ul,
ol {
  margin: 40px 0;
  padding-left: 40px; }
  ul:first-child,
  ol:first-child {
    margin-top: 0; }
  ul:last-child,
  ol:last-child {
    margin-bottom: 0; }
  ul li,
  ol li {
    padding-left: 10px;
